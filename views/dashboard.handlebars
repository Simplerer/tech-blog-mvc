<section class="dashboard">

    {{#each posts}}
    <a href="/post/edit/{{id}}">
    <div id="beside">
        <section class="home-posts">
            <div id="card-header">
                <h2>{{title}}</h2>
                <p>Posted {{format_date createdAt}}</p>
            </div>
            <p id="card-body">{{content}}</p>
        </section>
    </div>
    </a>
    {{/each}}

    <button id="post-it"><a href="/posts">Add Post</a></button>

</section>
