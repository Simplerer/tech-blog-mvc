<section class="dashboard">

    {{#each posts}}
    <div id="beside">
        <section class="home-posts">
            <div id="card-header">
                <h2>{{title}}</h2>
                <p>Here is a time stamp for that Post</p>
            </div>
            <p id="card-body">{{content}}</p>
        </section>
    </div>
    {{/each}}

    <button id="post-it"><a href="/posts">Add Post</a></button>

</section>
